<script>
        $(document).on("click", "#subBtn", function () {
            $("#errMes").empty()
            $.ajax({
                url: "/Home/Subscribe/",
                type: "get",
                data: {
                    "email": $("#subBtn").prev().val()
                },
                success: function (res) {
                    $("#errMes").append(res)

                }
            });

        });

</script>



